<%= render "shared/header" %>

<div class="contents row">
    <%# カメラ新規投稿 %>
  <div class="icon">
    <div class='purchase-btn1'>
      <a href="/songs/new">
        <%= image_tag 'カラオケ.jpg' , size: '120x120' ,class: "purchase-btn-icon" %>
      </a>
    </div>
  </div>
    <%# カメラ新規投稿 %>


  <% @songs.each do |song| %>
    <table class="detail-table">
      <tbody>
        <tr>
          <th class="detail-song">アーティスト名</th>
          <th class="detail-value">曲名</td>
        </tr>

        <tr>
          <td class="detail-song"><%= song.artist %></td> 
          <td class="detail-value">
            <%= link_to song_path(song.id) do %>
              <%= song.title %>
            <% end %>
          </td> 
        </tr>
      </tbody>
    </table>
  <% end %>
</div>

